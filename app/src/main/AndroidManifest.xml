<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mvp_application_java">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MVPApplicationJava"
        android:fullBackupContent="@xml/backup_descriptor">
        <activity
            android:name="com.example.mvp_application_java.ui.register_screen.RegisterActivity"
            android:exported="false" />
        <activity
            android:name="com.example.mvp_application_java.ui.welcome_screen.WelcomeActivity"
            android:exported="true"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.mvp_application_java.ui.dashboard.MainActivity"
            android:exported="false" />

        <activity
            android:name="com.example.mvp_application_java.ui.login_screen.LoginActivity"
            android:exported="false" />

    </application>

</manifest>